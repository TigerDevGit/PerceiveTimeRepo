<header>
  <div class="wrapper">
    <a href="/" class="logo">Toggl</a>
    <button class="nav-opener"></button>
    {{#if navLight }}
    <nav class="main-nav main-nav--light">
    {{ else }}
    <nav class="main-nav">
    {{/if}}
      <ol>
        <li><a href="/features">Features</a></li>
        <li><a href="/about">About</a></li>
        {{#if logged}}
        <li><a href="#" class="cta-button cta-button--small cta-button--inline cta-button--white-empty cta-button--no-arrow js-logout-button">Log out</a></li>
        <li><a href="/app/" clickthrough class="cta-button cta-button--small cta-button--inline cta-button--filled">Go to tracker</a></li>
        {{ else }}
        <li><a href="#" class="js-login-button">Log in</a></li>
        <li><a href="/signup" class="cta-button cta-button--small cta-button--inline cta-button--filled cta-button--no-arrow js-ripple-button"  ga-track="signup, nav_click">Sign up</a></li>
        {{/if}}
      </ol>
      <ol class="secondary">
        {{> 'secondary-nav' }}
      </ol>
    </nav>
  </div>
</header>
